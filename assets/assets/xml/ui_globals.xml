<?xml version="1.0" encoding="utf-8" ?>
<data>
    
    <inject name="point_def" />
    <!-- define points as if a 600 x 800 screen -->
    <!--<point_size value="screen.width*0.001667" x="screen.width*0.0016667" y="screen.width*0.0016667"/>-->
    
	<definition id="sans16c" size="16" style="bold" color="0xffffff" outline="0x000000" align="center"/>
	<definition id="sans32c" size="32" style="bold" color="0xffffff" outline="0x000000" align="center"/>
    
	<definition id="ui16" font="cartoonist" size="16" style="bold" color="0xffffff" border="outline" border_color="0xC0C0C0" border_size="1px" align="center"/>
	<definition id="ui24" font="cartoonist" size="24" style="bold" color="0xffffff" border="outline" border_color="0xC0C0C0" border_size="1px" align="center"/>
	<definition id="ui32" font="cartoonist" size="32" style="bold" color="0xffffff" border="outline" border_color="0xC0C0C0" border_size="1px" align="center"/>
    
	<definition id="title64" font="cartoonist" size="82" style="bold" color="0xFC5055" border="outline" border_color="white" border_size="1px" align="center"/>
    
    <definition id="ui_btn_lg" width="144pt" height="72pt" label="#BtnLg">
        <graphic id="all" image="ui/blank_button" slice9="20,20,48,48">
            <scale_src scale="0.5"/>
        </graphic>
        <param type="string" value="play"/>
        <text use_def="sans32c" color="0xffffff">
            <color id="up" value="0xffffff"/>
            <color id="over" value="0xffff00"/>
            <color id="down" value="0xffff00"/>
        </text>
    </definition>
    
    <definition id="ui_btn_md" width="120pt" height="46pt" label="#BtnMd">
        <graphic id="all" image="ui/blank_button" slice9="20,20,48,48">
            <scale_src scale="0.5"/>
        </graphic>
        <param type="string" value="play"/>
        <text use_def="sans16c" color="0xffffff">
            <color id="up" value="0xffffff"/>
            <color id="over" value="0xffff00"/>
            <color id="down" value="0xffff00"/>
        </text>
    </definition>
    
    <definition id="ui_btn_sm" width="100pt" height="36pt" label="#BtnSm">
		<graphic id="all" image="ui/blank_button" slice9="20,20,48,48">
			<scale_src scale="0.5"/>
		</graphic>
		<param type="string" value="controls"/>
		<text use_def="sans16c" color="0xffffff">
			<color id="up" value="0xffffff"/>
			<color id="over" value="0xffff00"/>
			<color id="down" value="0xffff00"/>
		</text>
	</definition>
    
    <definition id="close_btn" width="36pt" height="36pt">
		<graphic id="all" image="ui/close_button" slice9="24,24,50,50">
			<scale_src scale="0.5"/>
		</graphic>
        <param type="string" value="close"/>

	</definition>
</data>